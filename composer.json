{
    "name": "pawel-brzezinski/sulu-storage-bundle",
    "description": "Custom implements of filesystem for Sulu storage. This bundle replaces standard Sulu Media Storage filesystem.",
    "type": "sulu-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Paweł Brzeziński",
            "email": "pawel.brzezinski@smartint.pl"
        }
    ],
    "homepage": "https://github.com/pawel-brzezinski/sulu-storage-bundle",
    "autoload": {
        "psr-4": {
            "PB\\Bundle\\SuluStorageBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "require": {
        "php": "^7.1",
        "ext-fileinfo": "*",
        "ext-json": "*",
        "pawel-brzezinski/overlay-component": "^1.0",
        "oneup/flysystem-bundle": "^3.0",
        "sulu/sulu": "~1.6",
        "symfony/symfony": "^3.0"
    },
    "require-dev": {
        "jackalope/jackalope-doctrine-dbal": "^1.3",
        "phpunit/phpunit": "^5.7",
        "matthiasnoback/symfony-dependency-injection-test": "^1.2",
        "nyholm/symfony-bundle-test": "^1.4",
        "league/flysystem-aws-s3-v3": "^1.0",
        "league/flysystem-cached-adapter": "^1.0",
        "league/flysystem-replicate-adapter": "^1.0",
        "superbalist/flysystem-google-storage": "^7.2"
    }
}
